ted_timestamp":null,"tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1278878808602316840","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":31,"userName":"_diaso_"},{"id":"1278874720413945907","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"the reason is simply what it shows you in the present experience. exactly like a blueprint. the blueprint doesnt have to represent anything outside itself to be useful","timestamp":"2024-08-30T00:31:33.690000+00:00","edited_timestamp":null,"tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1278873476165271602","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":28,"userName":"_diaso_"},{"id":"1278872197355671663","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"im experiencing a mental drawing of something im not experiencing, aka thinking","timestamp":"2024-08-30T00:21:32.146000+00:00","edited_timestamp":null,"tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1278871514544078941","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":25,"userName":"_diaso_"},{"id":"1278868801399558238","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"> wouldn’t that then make dividing pointless?\n\nNot entirely. It becomes more of a blueprint or a \"mental drawing\". And it's also great for communication, language wouldn't exist without it. Language is entirely made of divisions.\n\nYou're not using parts of experience to make sense of experience anymore, it gets \"put in its place\", so to speak. It becomes an experience, not something that explains experience. \n\nThe previous usage of thought is like trying to use taste to explain the origin of your experience, instead of just enjoying the flavor of vanilla ice cream. Thought is a similar perception to the other senses, but trying to symbolize your entire experience goes beyond its use.\n\n> And if so wouldn’t that also crumble all knowledge we’ve ever gathered?\n\nyes, you aren't a symbol","timestamp":"2024-08-30T00:08:02.487000+00:00","edited_timestamp":"2024-08-30T00:13:02.396000+00:00","tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1278865438532702371","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":23,"userName":"_diaso_"},{"id":"1278863770634027028","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"the thoughts don't actually divide anything, in the same way that writing the word \"future\" on a whiteboard doesn't open a portal to the future. the thought apple isn't an actual apple, it's a symbol.\n\nthought's aren't excluded, they're included alongside experience. oneness is not rejecting parts of your experience, it's including all of it","timestamp":"2024-08-29T23:48:03.059000+00:00","edited_timestamp":null,"tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1278857607381127251","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":20,"userName":"_diaso_"},{"id":"1275611635486167164","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"All \"things\" are symbols of experience created in thinking, but those symbols are also experienced. Not a single thing you can think of is left out of experience, because you're experiencing thinking it. Thus there is no such thing as dividing experience into objects, as there is nothing in experience which can divide experience since it would also have to be experienced.\n\nVery simply, there aren't two components to pointing or representing. Either something is being represented, or it isn't. There's no such thing as \"something outside the representation\" because then there is no representation for something to be outside of.\n\nDistinctions are like mental fingers. They are never what they point at, they only consist of pointing. All distinctions are symbols because they refer to parts of experience and you only experience all of experience (otherwise it isn't experienced). Experience has no modality of separating itself, it only uses itself to point to itself, which is all still part of itself; being the one undivided thing that it is.\n\nThe finger and what the finger points at are the same occurrence. You can't have one without the other.\n\nAs an example, hammers don't represent hammers.\n\nSo when you say \"this is a hammer\"...\n\nThere is no \"this\" which is being a hammer, that's the pointer which isn't a hammer (like a hand which points to a hammer isn't a hammer, it's a hand). There's just the hammer. One thing. Not two things. There isn't something separate from the hammer which is being a hammer.\n\nFleshing it out further, there isn’t any part of experience being another part of experience. All words point to one thing, which is just your experience now. So there really is nothing other than experience which you can use to point at the experience, that is why you have to use parts of experience (words, fingers, thoughts) to point, otherwise the distinction would be self-evident and there would be no need for symbols to represent anything.","timestamp":"2024-08-21T00:25:13.618000+00:00","edited_timestamp":"2024-08-21T00:34:04.648000+00:00","tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1275568586542288896","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":18,"userName":"_diaso_"},{"id":"1275226738124918785","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"all objects are links between objects. that's how you make the distinction in the first place. \"car\" denotes objects that arent cars. \n\ncars dont describe cars, only the word \"car\" does. a symbol only describes things, it is never the thing itself. but the catch is that there is nothing to describe without the description either.","timestamp":"2024-08-19T22:55:46.936000+00:00","edited_timestamp":"2024-08-19T22:56:35.164000+00:00","tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1275218746096619601","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":5,"userName":"_diaso_"},{"id":"1275208825150115850","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"Yeah so we know that schematics aren't cars, and that the words in a dictionary aren't the actual things (the word cat isn't a cat), so the thoughts create a \"bridge\" by imagining the distinctions even though the distinctions don't exist outside it.\n\nRepresentations are never the thing they represent, but if you take the representation away, nothing is being represented anymore. Thus thought and thing being represented are the exact same, there's no difference, it's a false dichotomy. It's self-contained.\n\nThoughts dont represent things, they ***are*** the things they represent, it's a self-contained perception, exactly like seeing, hearing, touching, etc. There's nothing outside the thought being represented. Just like you can't see something outside of seeing.","timestamp":"2024-08-19T21:44:36.150000+00:00","edited_timestamp":"2024-08-19T21:47:01.927000+00:00","tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1275206666568335472","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":3,"userName":"_diaso_"},{"id":"1275200226592034968","channel_id":"1275195736589598831","author":{"id":"478359517181247490","username":"_diaso_","avatar":"66de051aedb999eec8e4233c11afd878","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"Diaso","avatar_decoration_data":null,"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":null,"primary_guild":null},"content":"try to find a representation without thinking, like:\n\n-something that represents a car\n\n-something that simulates the future\n\n-visual perceptions that categorize things\n\netc.\n\nIs there something outside of the thought that represents and categorizes things?","timestamp":"2024-08-19T21:10:26.094000+00:00","edited_timestamp":null,"tts":false,"mention_everyone":false,"mentions":[{"id":"617160836083089427","username":"steamedhams4974","avatar":"2fd24eb46640f2e5813dadbf3c8fb6b6","discriminator":"0","public_flags":0,"flags":0,"banner":null,"accent_color":null,"global_name":"steamed hams","avatar_decoration_data":{"asset":"a_b9f89cd88eabf437777bf35f55e6126f","sku_id":"1402472280642289775","expires_at":null},"collectibles":null,"display_name_styles":null,"banner_color":null,"clan":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"},"primary_guild":{"identity_guild_id":"185590609631903755","identity_enabled":true,"tag":"Ыend","badge":"3e1695b59c9f89e89258e7c42cc4ef5e"}}],"attachments":[],"embeds":[],"pinned":false,"type":19,"message_reference":{"type":0,"channel_id":"1275195736589598831","message_id":"1275195736589598831","guild_id":"1129343306485215342"},"flags":0,"components":[],"position":0,"userName":"_diaso_"}]